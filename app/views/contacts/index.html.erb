<h2>Contacts Index</h2>

<%= @contacts.each do |contact| %>
  <h3> First Name: <%=contact.first_name%></h3>
  <h3>Last Name:<%=contact.last_name%></h3>
  <p><b>Email:</b><%=contact.email%></p>
  <p><b>Phone Number:</b><%=contact.phone_number%></p>
  <p><b>Middle Name:</b><%=contact.middle_name%></p>
 <!--  <p><b>Latitude:</b><%=contact.latitude%></p>
  <p><b>Longitude:</b><%=contact.longitude%></p> -->
  <p><b>Bio:</b><%=contact.bio%></p>
  <p><b>User ID:</b><%=contact.user_id%></p>
  <a href="/contacts/<%=contact.id%>">More Info</a>
<% end %>
